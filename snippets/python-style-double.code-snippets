{
     "comment=block=sm": {
          "prefix": "//block=sm",
          "body": ["#===========================", "#      ${1:COMMENT BLOCK}", "#   ${2}", "#   ", "#==========================="],
          "description": "COMMENT BLOCK SMALL",
          "scope": ""
     },
     "comment=block=md": {
          "prefix": "//block=md",
          "body": [
               "#================================================",
               "#                ${1:COMMENT BLOCK}",
               "#   ${2}",
               "#   ",
               "#   ",
               "#   ",
               "#================================================"
          ],
          "description": "COMMENT BLOCK MEDIUM",
          "scope": ""
     },
     "comment=block=lg": {
          "prefix": "//block=lg",
          "body": [
               "#==========================================================================",
               "#                              ${1:COMMENT BLOCK}",
               "#   ${2}",
               "#   ",
               "#   ",
               "#   ",
               "#=========================================================================="
          ],
          "description": "COMMENT BLOCK LARGE",
          "scope": ""
     },
     "comment=block=xl": {
          "prefix": "//block=xl",
          "body": [
               "#===================================================================================================",
               "#                                            ${1:COMMENT BLOCK}",
               "#   ${2}",
               "#   ",
               "#   ",
               "#   ",
